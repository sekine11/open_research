<%= form_with(model: laboratory) do |f| %>
	<%= render 'layouts/error_messages', model: f.object %>
	<div class="form-group">
		<%= f.label :name, "名前" %>
		<%= f.text_field :name, class: "form-control" %>
	</div>
	<div class="form-group">
		<%= f.submit "編集する", class: "btn btn-success" %>
	</div>
<% end %>