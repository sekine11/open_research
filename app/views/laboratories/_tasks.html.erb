<% lab_tasks.each do |task| %>
	<div class="w-100 border-bottom">
		<div class="w-100 d-md-flex justify-content-between align-items-center py-2">
			<div class="task-status" id="task-status-<%= task.id %>">
				<%= render "laboratories/task_status", task: task %>
			</div>
			<p class="m-0"><%= task.content %></p>
			<p class="m-0"><%= task.create_time %></p>
			<div id="task-update-<%= task.id %>">
				<%= render "laboratories/task_update", task: task, laboratory: @laboratory %>
			</div>
		</div>
	</div>
<% end %>